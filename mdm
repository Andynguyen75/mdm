csrutil authenticated-root disable;
diskutil mount /Volumes/Macintosh\ HD;
 Mount -uv /Volumes/Macintosh\ HD;	
 cd "/Volumes/Macintosh\ HD/System/Library";
mv ~LaunchAgents/com.apple.ManagedClientAgent.agent.plist ~Templates/com.apple.ManagedClientAgent.agent.plist;
mv ~LaunchAgents/com.apple.ManagedClientAgent.enrollagent.plist ~Templates/com.apple.ManagedClientAgent.enrollagent.plist;
